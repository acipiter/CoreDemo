@model List<EntityLayer.Concrete.Category>

<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Kategory Adı</th>
        <th>Kategori Durumu</th>
        <th></th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.CategoryId</td>
            <td>@item.CategoryName</td>
            <td>@item.CategoryStatus</td>
            <td><button class="btn btn-info" href="#">Detaylar</button></td>
            <td><button class="btn btn-danger" href="#">Sil</button></td>
        </tr>
    }
</table>